SELECT [a].[Id], [a].[ParentId], [a].[Name], [a].[Url], [a].[icon], [a].[SortOrder], [t].[Id], [t].[ParentId], [t].[Name], [t].[Url], [t].[icon], [t].[SortOrder]
FROM [AdminNavItems] AS [a]
LEFT JOIN (
    SELECT [a0].[Id], [a0].[ParentId], [a0].[Name], [a0].[Url], [a0].[icon], [a0].[SortOrder]
    FROM [AdminNavItems] AS [a0]
    WHERE [a0].[Enabled] = CAST(1 AS bit)
) AS [t] ON ([a].[ParentId] <> [t].[ParentId] OR [a].[ParentId] IS NULL OR [t].[ParentId] IS NULL) AND ([a].[ParentId] IS NOT NULL OR [t].[ParentId] IS NOT NULL)
WHERE [a].[Enabled] = CAST(1 AS bit) AND [a].[ParentId] = 0
ORDER BY [a].[Id]

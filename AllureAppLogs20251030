SELECT [c].[Id], [c].[Cat_Name], [t].[Id], [t].[Name]
FROM [Categories] AS [c]
LEFT JOIN (
    SELECT [s].[Id], [s].[SubCat_Name] AS [Name], [s].[CatId]
    FROM [SubCategories] AS [s]
    WHERE [s].[Deleted] = CAST(0 AS bit)
) AS [t] ON [c].[Id] = [t].[CatId]
WHERE [c].[Deleted] = CAST(0 AS bit)
ORDER BY [c].[Id]
